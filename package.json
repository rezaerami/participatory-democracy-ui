    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0",
    "node": ">= 14.18.0"
  }
